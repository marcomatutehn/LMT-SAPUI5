<mvc:View
	controllerName="sap.ui.demo.walkthrough.controller.HelloPanel"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:html="http://www.w3.org/1999/xhtml"
	class="viewPadding">
	<Panel
		class="sapUiResponsiveMargin"
		width="auto">
		<l:VerticalLayout>
			<l:content>
			<FlexBox
				height="auto"
				alignItems="Start"
				justifyContent="Start">
				<items>
					<GenericTile 
					 class="sapUiTinyMarginBegin sapUiTinyMarginTop sapUiTinyMarginBottom tileLayout" 
					 header="Create New Account"
					 type="Navigation"
					 press="onPresscreateNewAccount">
					<TileContent footer="SAP">
						<ImageContent src="sap-icon://home-share" />
					</TileContent>
					</GenericTile>
					<GenericTile 
						 class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
						 header="Account Info Page"
						 type="Navigation"
						 press="onPressaccountInfoPage">
						<TileContent  footer="SAP">
							<ImageContent src="sap-icon://home-share" />
						</TileContent>
					</GenericTile>
				</items>
			</FlexBox>
			<FlexBox
				height="auto"
				alignItems="Center"
				justifyContent="Start">
				<items>
					<GenericTile 
						 class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" 
						 header="Trial Account Page"
						 type="Navigation"
						 press="onPresstrialAccountPage">
						<TileContent footer="Current Quarter">
							<ImageContent src="sap-icon://home-share" />
						</TileContent>
					</GenericTile>
				</items>
			</FlexBox>
				

				<!--<l:VerticalLayout
						width="100%"
						class="sapUiContentPadding nestedFlexboxes">
					<l:content
							containerQuery="true"
							defaultSpan="L6 M6 S6"	
